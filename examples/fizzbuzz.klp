def fizz = [i: i32]: String (
  i % 5 > match (
    0 | ( "" )
    _ | ( "fizz" )
  )
)

def buzz = [i: i32]: String (
  i > tree (
    i % 3 ?> 0 | ( "" )
    _ | ( "buzz" )
  )
)

def fizzbuzz = [iteration: i32] (
  iteration #> [fizz, buzz] + "\n" > print
)

def main = [] (
  fizzbuzz # [0..101]
)
